<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" minHeight="150.0" minWidth="300.0" prefHeight="512.0" prefWidth="1024.0" spacing="20.0" xmlns="http://javafx.com/javafx/1.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bafkit.cloud.storage.client.controllers.CloudStorageController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <HBox alignment="CENTER" prefHeight="28.0" prefWidth="200.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="101.0" text="Cload Storage" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="cloudFilesList" onMouseClicked="#selectItem" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
        </VBox>
        <Button fx:id="button" mnemonicParsing="false" onAction="#clickExit" prefWidth="80.0" text="Exit" textAlignment="CENTER" VBox.vgrow="ALWAYS" />
    </children>

</VBox>
